{
    "compilerOptions": {
        "baseUrl": "./src",
        "target": "es5",
        "module": "umd",
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noImplicitThis": true,
        "noImplicitUseStrict": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "rootDir": "src",
        "outDir": "dist"
    }
}
